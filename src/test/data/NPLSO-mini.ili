INTERLIS 2.3;

MODEL NPLSO (de)
   AT "mailto:stefan.ziegler@bd.so.ch" VERSION "2019-05-26" =
   IMPORTS SO_NPLSO_FunctionsExt;
   
  DOMAIN
    Coord2 = COORD
      2460000.000 .. 2870000.000,
      1045000.000 .. 1310000.000,
      ROTATION 2 -> 1;

  TOPIC Vorschriften =
    
    CLASS Dokument =
      TextImWeb : TEXT*1024;
      Titel : TEXT*1024;
    END Dokument;
    
    ASSOCIATION HinweisWeitereDokumente =
      Ursprung -- {0..*} Dokument;
      Hinweis -- {0..*} Dokument;
    END HinweisWeitereDokumente;  
    
  END Vorschriften;

  TOPIC Nutzungsplanung =
    
    CLASS Typ_Grundnutzung =
      Bezeichnung : TEXT*80;
      Typ_Kt : TEXT*4;
      Code_kommunal : TEXT*12;
    END Typ_Grundnutzung;
 
    CLASS Grundnutzung =
      Bemerkungen : TEXT*80;
      Geometrie : SURFACE WITH (STRAIGHTS, ARCS) VERTEX Coord2 WITHOUT OVERLAPS > 0.002;
    END Grundnutzung;
    
    ASSOCIATION Typ_Grundnutzung_Grundnutzung =
      Typ_Grundnutzung -<> {1} Typ_Grundnutzung;
      Grundnutzung -- {0..*} Grundnutzung;
    END Typ_Grundnutzung_Grundnutzung;
    
  END Nutzungsplanung;


END NPLSO.